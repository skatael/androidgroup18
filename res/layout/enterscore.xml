<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:orientation="vertical" android:paddingLeft="30px" android:paddingRight="30px" android:paddingTop="10px" android:layout_height="match_parent">
    <TextView android:layout_height="wrap_content" android:id="@+id/textView1" android:layout_width="wrap_content" android:text="@string/congrats"></TextView>
    <EditText android:id="@+id/editText1" android:layout_height="wrap_content" android:layout_width="match_parent" android:text="@string/yourname" android:layout_marginTop="20px"></EditText>
    <Button android:layout_marginTop="40px" android:id="@+id/enterscorebutton" android:layout_height="wrap_content" android:layout_width="match_parent" android:onClick="highscorelist" android:text="@string/submit"></Button>
</LinearLayout>
